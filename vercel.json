{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.js"
    }
  ],
  "env": {
    "PORT": "3001",
    "BASE_URL": "https://images-movie-store.s3.sa-east-1.amazonaws.com"
  }
}
